					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status is 200 or 201\", () => {\r",
							"  pm.expect(pm.response.code).to.be.oneOf([200, 201]);\r",
							"});\r",
							"\r",
							"var jsonData = {};\r",
							"try { jsonData = pm.response.json(); } catch(e) {}\r",
							"pm.test(\"Response contains firstName\", () => {\r",
							"  pm.expect(jsonData.firstName).to.eql(\"Oleksandra\");\r",
							"});\r",
							"pm.test(\"Response contains id\", () => {\r",
							"  pm.expect(jsonData.id).to.exist;\r",
							"});\r",
							"\r",
							"// save id to environment for later tests\r",
							"if (jsonData.id) {\r",
							"  pm.environment.set(\"createdUserId\", jsonData.id);\r",
							"}\r",
							""
						],
